void setup(void) {

  Serial.begin(9600);
  Wire.begin();
  sensor.setTimeout(500);
  if (!sensor.init())
  {
    Serial.println("Failed to detect and initialize sensor!");
    while (1) {}
  }

  //set mode motor driver
  pinMode(IN1, OUTPUT);
  pinMode(IN2, OUTPUT);
  pinMode(IN3, OUTPUT);
  pinMode(IN4, OUTPUT);

  sensor.startContinuous();
  
  u8g2.begin();  
  

  u8g2.clearBuffer();          
  u8g2.drawXBM(0,0, 128, 64, MDlogo);
  u8g2.sendBuffer(); 
   
}


void loop(void) {


  //start DEST

  if(checkinterupt()==0){
    
    if(movedirection==1){
     //forward
      Serial.print("MOVE FORWARD / ");
      Serial.println(moveduration);
       Move_Forward();
       setexpression(1);
      
    }else if(movedirection==2){
     //backward
     Serial.print("MOVE BACKWARD / ");
     Serial.println(moveduration);
       Move_Reverse();
       setexpression(1);
     
    }else if(movedirection==3){
     //left
     Serial.print("TURN LEFT / ");
     Serial.println(moveduration);
       Move_Left();
       setexpression(2);
    
    }else if(movedirection==4){
     //righ
     Serial.print("TURN RIGHT / ");
     Serial.println(moveduration);
       Move_Right();
       setexpression(3);
     
    }else if(movedirection==0){
     //stop
     Serial.print("STOP / ");
     Serial.println(moveduration);
       if(moveduration%3==1){
        setexpression(1);
       }else if(moveduration%3==2){
        setexpression(2);
       }else if(moveduration%3==0){
        setexpression(3);
       }
    
    }

  }
  
  delay(500);
  //Move_Stop();
  recordhistory(sensor.readRangeContinuousMillimeters());
  if(checkinterupt()==1){
    interuptaction();
  }else if(moveduration==0){
    randombehave();
  }else{
    moveduration=moveduration-1;
  }
  
  //sensor.readRangeContinuousMillimeters()
}


void Move_Forward()
{
  digitalWrite(IN1, LOW);//backward kanan
  digitalWrite(IN2, HIGH);//forward kanan

  digitalWrite(IN4, LOW);//backward kiri  
  digitalWrite(IN3, HIGH);//forward kiri
}

void Move_Reverse()
{
  digitalWrite(IN2, LOW);//forward kanan
  digitalWrite(IN1, HIGH);//backward kanan
  
  digitalWrite(IN3, LOW);//forward kiri
  digitalWrite(IN4, HIGH);//backward kiri 
}

void Move_Right()
{
  digitalWrite(IN2, LOW);//forward kanan
  digitalWrite(IN1, HIGH);//backward kanan

  digitalWrite(IN4, LOW);//backward kiri 
  digitalWrite(IN3, HIGH);//forward kiri
}

void Move_Left()
{
  digitalWrite(IN1, LOW);//backward kanan
  digitalWrite(IN2, HIGH);//forward kanan
  
  digitalWrite(IN3, LOW);//forward kiri
  digitalWrite(IN4, HIGH);//backward kiri 
}

void Move_Stop()
{
  digitalWrite(IN1, LOW);//backward kanan
  digitalWrite(IN2, LOW);//forward kanan
  
  digitalWrite(IN3, LOW);//forward kiri
  digitalWrite(IN4, LOW);//backward kiri 
}

void randombehave(){
  int behave=random(1,6);
  
  if(behave==1){
   //forward
     movedirection=1;
     moveduration=random(1,5);
  }else if(behave==2){
   //backward
     movedirection=2;
     moveduration=random(1,3);
  }else if(behave==3){
   //left
     movedirection=3;
     moveduration=random(1,4);
  }else if(behave==4){
   //righ
     movedirection=4;
     moveduration=random(1,4);
  }else if(behave==5){
   //stop
     movedirection=0;
     moveduration=random(1,7);
  }
}

int checkinterupt(){
  Serial.println(sensor.readRangeContinuousMillimeters());
  if(sensor.readRangeContinuousMillimeters()>maxdistancewall || (sensor.readRangeContinuousMillimeters()<mindistancewall && sensor.readRangeContinuousMillimeters()>touchchangeindicatormargin) ){
    //obstacel ahead
    Serial.println("obstacel ahead");
    interuptype=1;
    return 1;
  }else if(sensor.readRangeContinuousMillimeters()<=touchchangeindicatormargin){
    //hand ahead
    Serial.println("hand ahead");
    interuptype=2;
    return 1;
  }
  return 0;
}

void interuptaction(){
  if(interuptype==1){
    //do when theres a obstacle
    int avoidaction=random(1,5);
  
    if(avoidaction==1 || avoidaction==3){
