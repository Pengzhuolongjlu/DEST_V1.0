       Move_Right();
       setexpression(5);
          while(sensor.readRangeContinuousMillimeters()>maxdistancewall || (sensor.readRangeContinuousMillimeters()<mindistancewall && sensor.readRangeContinuousMillimeters()>touchchangeindicatormargin)){
          delay(500);
          }
    }
    
  }else if(interuptype==2){
    //do when hand in fron sensor
    Move_Stop();
    setexpression(random(7,12));
    while(sensor.readRangeContinuousMillimeters()<=touchchangeindicatormargin){
      delay(500);
    }
  }
}


